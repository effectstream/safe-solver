{
  "name": "@safe-solver/node",
  "version": "0.3.0",
  "license": "MIT",
  "exports": {
    ".": "./src/main.ts"
  },
  "tasks": {
    "check": "deno check --unstable-raw-imports src/main.dev.ts && deno check --unstable-raw-imports src/main.testnet.ts",
    
    "node:start:dev": "deno run -A --inspect --unstable-raw-imports src/main.dev.ts",
    "node:start:testnet": "deno run -A --inspect --unstable-raw-imports src/main.testnet.ts",

    "dev": "deno task -f @paimaexample/tui clean && EFFECTSTREAM_ENV=dev NODE_ENV=development deno run -A --check --unstable-raw-imports scripts/start.dev.ts",
    "testnet": "deno task -f @paimaexample/tui clean && EFFECTSTREAM_ENV=testnet NODE_ENV=development deno run -A --check --unstable-raw-imports scripts/start.testnet.ts",

    "test": "deno run -A --unstable-raw-imports --check scripts/e2e.test.ts"
  },
  "imports": {
    "@paimaexample/concise": "jsr:@paimaexample/concise@0.3.127",
    "@paimaexample/config": "jsr:@paimaexample/config@0.3.127",
    "@paimaexample/log": "jsr:@paimaexample/log@0.3.127",
    "@paimaexample/orchestrator": "jsr:@paimaexample/orchestrator@0.3.127",
    "@paimaexample/runtime": "jsr:@paimaexample/runtime@0.3.127",
    "@paimaexample/tui": "jsr:@paimaexample/tui@0.3.127",
    "@paimaexample/utils": "jsr:@paimaexample/utils@0.3.127",
    "@paimaexample/sm": "jsr:@paimaexample/sm@0.3.127",
    "@paimaexample/coroutine": "jsr:@paimaexample/coroutine@0.3.127",
    "@paimaexample/db": "jsr:@paimaexample/db@0.3.127",
    "fastify": "npm:fastify@^5.4.0",
    "pg": "npm:pg@^8.14.0",
    "@sinclair/typebox": "npm:@sinclair/typebox@^0.34.30",
    "effection": "npm:effection@^3.5.0",
    "@paimaexample/explorer": "npm:@paimaexample/explorer@0.3.127",
    "@midnight-ntwrk/onchain-runtime": "npm:@midnight-ntwrk/onchain-runtime-v1@1.0.0-alpha.5"
  }
}