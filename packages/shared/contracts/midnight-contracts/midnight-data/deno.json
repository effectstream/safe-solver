{
  "name": "@safe-solver/midnight-contract-midnight-data",
  "exports": {
    ".": "./src/_index.ts",
    "./witnesses": "./src/witnesses.ts",
    "./contract": "./src/managed/midnight-data/contract/index.cjs"
  },
  "tasks": {
    "compact": "compact compile src/midnight-data.compact src/managed/midnight-data && deno task convert:js",
    "convert:js": "deno run -A convert-js.ts src/managed/midnight-data/contract/index.cjs src/managed/midnight-data/contract/index.ts",
    "contract:compile": "deno task compact"
  },
  "imports": {
    "vitest": "npm:vitest@^3.2.4"
  }
}